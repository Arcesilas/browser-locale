{
    "name": "arcesilas/browser-locale",
    "description": "Detect browser locale and guess which is most appropriate",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arcesilas",
            "email": "arcesilas@neutre.email"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
